    "hero": {
        "title": "Control working time. Boost team efficiency.",
        "description": "KickAider automatically tracks employee activity, records lateness and idle time - no extra questions or manual reports.",
        "button": "Login"
    },
    "aboutCompany": {
        "title": "About Company",
        "text": "KickAider is an employee working time monitoring system. The agent is installed on the computer once and collects activity data in background: what applications and websites are open, whether there are mouse movements and keystrokes. The manager sees everything in a convenient web panel - anytime, from any device.",
        "description": "We created KickAider for teams that value transparency: without real-time tracking, only objective data on working time."
    },
    "features": {
        "title": "Boost Team Productivity",
        "description": "Learn how working time is actually used. KickAider collects data automatically and turns it into clear reports - without surveys, tables and guesses.",
        "cards": {
            "basicReports": {
                "title": "Basic Reports",
                "description": "Summary analytics for the whole company: top applications and sites, active time and idle time ratio. Filter by departments and employees."
            },
            "activityHistory": {
                "title": "Activity History",
                "description": "Timeline of each employee down to the minute. Screenshots are recorded automatically when switching applications — the picture of the day can always be restored."
            },
            "ratingAndDetails": {
                "title": "Rating and Day Details",
                "description": "Employee performance ratings, detailed breakdown of each workday: when they arrived, how long they were active, what applications they used."
            },
            "insightsAndRecommendations": {
                "title": "Insights and Recommendations",
                "description": "The system identifies productivity patterns, shows time loss areas and offers specific recommendations for team efficiency growth."
            }
        }
    },
    "carousel": {
        "title": "Everything Under Control - in Real Time",
        "text": "Employee online statuses, alerts for lateness or long idle time, working time report based on schedule - KickAider provides a full picture without extra actions."
    },
    "clients": {
        "title": "Trusted by Leaders",
        "text": "KickAider is used by companies that value honest working time analytics - without complex settings and IT departments."
    },
    "faq": {
        "title": "Frequently Asked Questions",
        "description": "Answers to the most popular questions about KickAider.",
        "questions": [
            {
                "question": "How is the agent installed on the employee's computer?",
                "answer": "The agent is installed once on the employee's Windows computer. After installation, it works in the background and automatically sends data to the company server."
            },
            {
                "question": "Do employees see they are being observed?",
                "answer": "This is configurable. The agent can work in open mode (employees know about the system) or stealth mode - at the manager's discretion."
            },
            {
                "question": "Is the data stored on KickAider servers?",
                "answer": "No. KickAider is deployed on your company infrastructure. All data is stored with you - we have no access to it."
            },
            {
                "question": "What operating systems are supported?",
                "answer": "The agent works on Windows. The web dashboard for the manager is available through any browser on any device."
            }
        ]
    },
